<main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <section id="breadcrumbs" class="breadcrumbs">
      <div class="breadcrumb-hero">
        <div class="container">
          <div class="breadcrumb-hero">
            <h2>Users Profiles</h2>
            <p>List of Users Profiles</p>
          </div>
        </div>
      </div>
      <div class="container" id="addContainer">
        <ol>
          <li><a href="/">Home</a></li>
          <li>Users Profiles</li>
        </ol>
     
    </div>
    </section><!-- End Breadcrumbs -->
  
    <!-- ======= Pricing Section ======= -->
    <section id="userProfiles" class="pricing">
      <div class="container">
       
        <div class="row">
          <% userProfiles.forEach(function(userProfile) { %>
  
          <div class="col-lg-3 col-md-6">
           
            <div class="box" data-aos="fade-up">
              <h3 id="h3"><a href="/userProfile/detail?id=<%= userProfile._id %>"><%= userProfile.user.firstName + " " +userProfile.user.lastName %></a></h3>
              <h4><sup></sup><span> </span></h4>
              <ul>
                <li id="li"><img id="indexImageSize" src="../img/<%= userProfile.userImage %>"/> </li>
                <li id="li"> <%= userProfile.nationality %></li>
                <li id="li"> <%= userProfile.favoriteTeam %></li>
                <li id="li"> <%= userProfile.favoritePlayer %></li>
                <p id="li"> <%= moment(userProfile.createdAt).fromNow() %> </p>
                <p id="li"> <%= moment(userProfile.updatedAt).fromNow() %> </p>

  
  
                
              </ul>

              <div class="btn-wrap">
                <a href="/userProfile/edit?id=<%= userProfile._id %>" class="btn-buy">Edit</a>
                <a href="/userProfile/delete?id=<%= userProfile._id %>" class="btn-buy">Delete</a>
              </div>
            </div>
          </div>
  
  
          <% }) %>
        </div>
       
      </div>
     
    </section><!-- End Pricing Section -->
  
   

  
  </main><!-- End #main -->
  
  