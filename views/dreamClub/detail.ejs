
  <main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <section id="breadcrumbs" class="breadcrumbs">
      <div class="breadcrumb-hero">
        <div class="container">
          <div class="breadcrumb-hero">
            <h2>Dream Club Details</h2>
           
          </div>
        </div>
      </div>
      <div class="container">
        <ol>
          <li><a href="/dreamClub/index">Dream Club</a></li>
          <li>details</li>
        </ol>
      </div>
    </section><!-- End Breadcrumbs -->

    <!-- ======= Team Section ======= -->
    <section id="team" class="team">
      <div class="container">

        <div class="row">

          <div class="col-lg-4 col-md-6 d-flex align-items-stretch">
            <div class="member" data-aos="fade-up">
              <div class="member-img">
        
              <div class="member-info">
                <h4 id="h4"><%= dreamClub.club.name %></h4>
                <p id="fontSize">  <img id="indexImageSize" src="../img/<%= dreamClub.dreamClubImage %>"/></p>
                <p id="fontSize"> Player Name: <%= dreamClub.player.name %> </p>
                <p id="fontSize"> Kit Number: <%= dreamClub.kitNumber %> </p>
                <p id="fontSize"> Player Position: <%= dreamClub.playerPosition %> </p>
                <p id="fontSize"> <a href="<%= dreamClub.playerVideo %>" target="_blank"> Player Video Link</a> </p>
                <p id="fontSize"> Created At: <%= moment(dreamClub.createdAt).fromNow() %> </p>
                <p id="fontSize"> Updated At: <%= moment(dreamClub.updatedAt).fromNow() %> </p>
                <p id="fontSize"> created By: <%=currentUser.firstName%> <%=currentUser.lastName%> </p>
                
              </div>
            </div>
          </div>

          

        </div>

      </div>
    </section><!-- End Team Section -->

  </main><!-- End #main -->

  