<main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <section id="breadcrumbs" class="breadcrumbs">
      <div class="breadcrumb-hero">
        <div class="container">
          <div class="breadcrumb-hero">
            <h2>Users</h2>
            <p>List of Users </p>
          </div>
        </div>
      </div>
      <div class="container" id="addContainer">
        <ol>
          <li><a href="/">Home</a></li>
          <li>Users</li>
        </ol>
     
    </div>
    </section><!-- End Breadcrumbs -->
  
    <!-- ======= Pricing Section ======= -->
    <section id="users" class="pricing">
      <div class="container">
       
        <div class="row">
          <% users.forEach(function(user) { %>
  
          <div class="col-lg-3 col-md-6">
           
            <div class="box" data-aos="fade-up">
              <h3 id="h3"><a href="/auth/detail?id=<%= user._id %>"><%= user.firstName + " " +user.lastName %></a></h3>
              <h4><sup></sup><span> </span></h4>
              <ul>
                <li id="li"> <%= user.emailAddress %></li>
                <li id="li"> <%= user.role %></li>
                <p id="li"> <%= moment(user.createdAt).fromNow() %> </p>
                <p id="li"> <%= moment(user.updatedAt).fromNow() %> </p>

  
  
                
              </ul>
          
              <div class="btn-wrap">
                <a href="/auth/edit?id=<%= user._id %>" class="btn-buy">Edit</a>
                <a href="/auth/delete?id=<%= user._id %>" class="btn-buy">Delete</a>
              </div>
            </div>
          </div>
  
  
          <% }) %>
        </div>
       
      </div>
     
    </section><!-- End Pricing Section -->
  
   

  
  </main><!-- End #main -->
  
  