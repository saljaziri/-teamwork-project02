<main id="main">

    <!-- ======= Breadcrumbs ======= -->
    <section id="breadcrumbs" class="breadcrumbs">
      <div class="breadcrumb-hero">
        <div class="container">
          <div class="breadcrumb-hero">
            <h2>User Details</h2>
           
          </div>
        </div>
      </div>
      <div class="container">
        <ol>
          <li><a href="/auth/index">Users</a></li>
          <li>details</li>
        </ol>
      </div>
    </section><!-- End Breadcrumbs -->

    <!-- ======= Team Section ======= -->
    <section id="team" class="team">
      <div class="container">

        <div class="row">

          <div class="col-lg-4 col-md-6 d-flex align-items-stretch">
            <div class="member" data-aos="fade-up">

              <div class="member-info">
                <h4 id="h4"><%= user.firstName + " " + user.lastName %></h4>
                <p id="fontSize"> User Email: <%= user.emailAddress %></p>
                <p id="fontSize"> Role: <%= user.role %> </p>
                <p id="fontSize"> Created At: <%= moment(user.createdAt).fromNow() %> </p>
                <p id="fontSize"> Updated At: <%= moment(user.updatedAt).fromNow() %> </p>
                
              </div>
            </div>
          </div>

          

        </div>

      </div>
    </section><!-- End Team Section -->

  </main><!-- End #main -->

  